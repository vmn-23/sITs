import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        in.useLocale(Locale.US);
        double x = in.nextDouble();
        double y = in.nextDouble();
        double s = 2-x*x;
        System.out.println ((y<s)&&((y>x)||(y>0)&&(x>0))? "YES": "NO");
    }
}
